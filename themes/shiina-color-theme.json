            },
            "name": "Markup.heading"
        },
        {
            "scope": ["markup.quote"],
            "settings": {
                "foreground": "#22863a"
            },
            "name": "Markup.quote"
        },
        {
            "scope": ["markup.italic"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#000000"
            },
            "name": "Markup.italic"
        },
        {
            "scope": ["markup.bold"],
            "settings": {
