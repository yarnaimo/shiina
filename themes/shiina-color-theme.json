    "tokenColors": [
        {
            "scope": ["meta.object-literal.key", "variable.other.property"],
            "settings": {
                "foreground": "#A1A200"
            },
            "name": "key"
        },
        {
            "scope": [
                "meta.brace",
                "keyword.operator.assignment",
                "keyword.operator.relational",
                "keyword.operator.type",
                "punctuation",
                "punctuation.definition.typeparameters",
                "meta.objectliteral punctuation.definition.block"
            ],
            "settings": {
                "foreground": "#7E939E"
            },
            "name": "op"
        },
        {
            "scope": [
                // "punctuation.definition.block",
                "punctuation.definition.typeparameters",
                "punctuation.definition.parameters"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#A1B0B8"
            },
            "name": "op"
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#7E939E"
            },
            "name": "Comment"
        },
        {
            "scope": ["constant"],
            "settings": {
                "foreground": "#b86fb8"
            },
            "name": "Constant"
        },
        {
            "scope": [
                "meta.function-call support.function",
                "meta.function-call entity.name.function"
            ],
            "settings": {
                "foreground": "#96825D"
            }
        },
        {
            "scope": [
                "entity.name.constant",
                "meta.definition.variable",
                // "variable.other.constant",
                "variable.language"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#434E55"
            },
            "name": "Constant"
        },
        // {
        //     "scope": ["keyword.operator.symbole", "keyword.other.mark"],
        //     "name": "Clojure workaround; don't highlight these separately from their enclosing scope",
        //     "settings": {
        //         "foreground": "#000000"
        //     }
        // },
        {
            "scope": ["entity.name.type", "support.type"],
            "settings": {
                "foreground": "#27A9AD"
            },
            "name": "Entity"
        },
        {
            "scope": ["entity.name.type.module"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#3CB9BC"
            },
            "name": "Entity"
        },
        // {
        //     "scope": ["variable.parameter.function"],
        //     "settings": {
        //         "foreground": "#000000"
        //     }
        // },
        {
            "scope": ["entity.name.tag"],
            "settings": {
                "foreground": "#22863a"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.optional",
                "punctuation.accessor.optional"
            ],
            "settings": {
                "foreground": "#E84174"
            },
            "name": "Keyword"
        },
        {
            "scope": ["storage", "storage.type"],
            "settings": {
                "foreground": "#E84174"
            },
            "name": "Storage"
        },
        {
            "scope": [
                "storage.modifier.package",
                "storage.modifier.import",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string",
                "string punctuation.section.embedded source"
            ],
            "settings": {
                "foreground": "#2386AF"
            },
            "name": "String"
        },
        {
            "name": "Ada workaround; don't highlight imports as strings",
            "scope": ["string.unquoted.import.ada"],
            "settings": {}
        },
        {
            "scope": ["support"],
            "settings": {
                "foreground": "#99835B"
            },
            "name": "Support"
        },
        {
            "scope": ["meta.property-name"],
            "settings": {
                "foreground": "#99835B"
            }
        },
        {
            "scope": ["variable.parameter"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E29C00"
            },
            "name": "Variable"
        },
        {
            "scope": ["variable.other"],
            "settings": {
                "foreground": "#434E55"
            }
        },
        {
            "scope": ["invalid.broken"],
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#b31d28"
            },
            "name": "Invalid - Broken"
        },
        {
            "scope": ["invalid.deprecated"],
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#b31d28"
            },
            "name": "Invalid – Deprecated"
        },
        {
            "scope": ["invalid.illegal"],
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#b31d28"
            },
            "name": "Invalid – Illegal"
        },
        {
            "scope": ["carriage-return"],
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#E84174"
            },
            "name": "Carriage Return"
        },
        {
            "scope": ["invalid.unimplemented"],
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#b31d28"
            },
            "name": "Invalid - Unimplemented"
        },
        {
            "scope": ["message.error"],
            "settings": {
                "foreground": "#b31d28"
            }
        },
        {
            "scope": ["string source"],
            "settings": {
                "foreground": "#000000"
            },
            "name": "String embedded-source"
        },
        {
            "scope": ["string variable"],
            "settings": {
                "foreground": "#99835B"
            },
            "name": "String variable"
        },
        {
            "scope": ["source.regexp", "string.regexp"],
            "settings": {
                "foreground": "#032f62"
            },
            "name": "String.regexp"
        },
        {
            "scope": [
                "string.regexp.character-class",
